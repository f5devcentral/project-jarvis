{
  "name": "project-jarvis",
  "version": "0.1.0",
  "description": "PSCLI",
  "main": "index.js",
  "bin": {
    "jarvis": "./dist/cli.js"
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' 'src/tests/**/*.{js,ts,tsx}'",
    "test": "nyc mocha -r ts-node/register \"tests/**/*.test.ts\"",
    "debug": "node -r ts-node/register --inspect src/cli.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "build-code-docs": "typedoc --out code_docs src",
    "build-package": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f5devcentral/project-jarvis.git"
  },
  "keywords": [
    "cli",
    "f5",
    "f5-corkscrew",
    "f5-conx-core"
  ],
  "author": "DumbySquare",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/f5devcentral/project-jarvis/issues"
  },
  "homepage": "https://github.com/f5devcentral/project-jarvis#readme",
  "nyc": {
    "check-coverage": false,
    "lines": 50,
    "functions": 50,
    "branches": 50,
    "statements": 50,
    "reporter": [
      "html",
      "text"
    ],
    "cache": false
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "mocha": {
    "timeout": 10000
  },
  "devDependencies": {
    "@types/assert": "^1.5.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "assert": "^2.0.0",
    "eslint": "^7.23.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "f5-conx-core": "github:dumpysquare/f5-conx-core",
    "f5-corkscrew": "github:f5devcentral/f5-corkscrew",
    "yargs": "^16.2.0"
  }
}
